#include <iostream>
#include <string>
#include <cctype>
using namespace std;

int main() {
    string texto;
    cout << "Ingrese el texto: ";
    getline(cin, texto);
    
    string sinVocales = "";
    for (char c : texto) {
        char lower = tolower(c);
        if (lower != 'a' && lower != 'e' && lower != 'i' && lower != 'o' && lower != 'u') {
            sinVocales += c;
        }
    }
    
    cout << "Salida: " << sinVocales << endl;
    return 0;
}
