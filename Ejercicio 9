#include <iostream>
#include <string>
#include <set>
using namespace std;

int main() {
    string A, B;
    cout << "Conjunto A (separado por comas): ";
    getline(cin, A);
    cout << "Conjunto B (separado por comas): ";
    getline(cin, B);
    
    set<char> setB;
    for (char c : B) {
        if (c != ' ' && c != ',') setB.insert(tolower(c));  // Ignorar case? Ejemplo mezcla
    }
    
    string C = "";
    set<char> uniques;  // Para no duplicar en C
    for (char c : A) {
        if (c != ' ' && c != ',') {
            char lower = tolower(c);
            if (setB.count(lower) && !uniques.count(lower)) {
                uniques.insert(lower);
                if (!C.empty()) C += ",";
                C += lower;  // Salida en lower por ejemplo
            }
        }
    }
    
    cout << "Salida: C = " << C << endl;
    return 0;
}
