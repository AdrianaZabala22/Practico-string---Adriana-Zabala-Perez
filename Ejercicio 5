#include <iostream>
#include <string>
using namespace std;

int main() {
    string cadena1, cadena2;
    cout << "Cadena 1: ";
    cin >> cadena1;
    cout << "Cadena 2: ";
    cin >> cadena2;
    
    string mezcla = cadena1 + cadena2;
    cout << "Salida: " << mezcla << "." << endl;  // Agrego punto como en ejemplo
    return 0;
}
